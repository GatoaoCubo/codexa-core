{
  "version": "1.1.0",
  "description": "Patterns to ignore during indexing",
  "patterns": [
    "**/node_modules/**",
    "**/.venv/**",
    "**/venv/**",
    "**/__pycache__/**",
    "**/.git/**",
    "**/.github/**",
    "*.pyc",
    "*.pyo",
    "*.pyd",
    "*.so",
    "*.dll",
    "*.exe",
    "*.log",
    "*.tmp",
    "*.temp",
    "*.bak",
    "*.swp",
    "*.swo",
    ".DS_Store",
    "Thumbs.db",
    "desktop.ini",
    "**/*.egg-info/**",
    "**/dist/**",
    "**/build/**",
    "**/.pytest_cache/**",
    "**/.mypy_cache/**",
    "**/.ruff_cache/**",
    "**/coverage/**",
    "**/.coverage",
    "**/htmlcov/**",
    "*.min.js",
    "*.min.css",
    "*.map",
    "package-lock.json",
    "yarn.lock",
    "pnpm-lock.yaml",
    ".env",
    ".env.*",
    "*.sqlite",
    "*.db",
    "**/screenshots/**",
    "**/outputs/**/*.json",
    "**/logs/**"
  ],
  "max_file_size_bytes": 1048576,
  "max_files_per_directory": 1000
}
